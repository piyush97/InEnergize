{
  "name": "@inergize/ai-service",
  "version": "1.0.0",
  "description": "AI Integration Service for InErgize Platform - GPT-4 Profile Optimization & Content Generation",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --config=coverage.config.js",
    "test:coverage:watch": "jest --coverage --watch --config=coverage.config.js",
    "test:coverage:ci": "jest --coverage --config=coverage.config.js --ci --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "openai",
    "gpt-4",
    "content-generation",
    "profile-optimization",
    "microservice",
    "inergize"
  ],
  "author": "InErgize Team",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.11.2",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.7.9",
    "ioredis": "^5.4.1",
    "winston": "^3.17.0",
    "prom-client": "^14.2.0",
    "openai": "^4.73.1",
    "joi": "^17.13.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^30.0.5",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "@types/jest": "^30.0.0",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": "ts",
    "ignore": ["src/**/*.test.ts"],
    "exec": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/inergize.git",
    "directory": "services/ai-service"
  }
}