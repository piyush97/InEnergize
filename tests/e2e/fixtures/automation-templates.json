{
  "connectionTemplates": {
    "professional": {
      "id": "connection_template_1",
      "name": "Professional Introduction",
      "category": "networking",
      "message": "Hi {{firstName}}, I came across your profile and was impressed by your experience in {{industry}}. I'd love to connect and learn more about your work at {{company}}. Looking forward to connecting!",
      "variables": ["firstName", "industry", "company"],
      "successRate": 85.3,
      "usageCount": 1250,
      "complianceScore": 95,
      "tags": ["professional", "networking", "generic"]
    },
    "industry_specific": {
      "id": "connection_template_2",
      "name": "Tech Industry Connection",
      "category": "industry_specific",
      "message": "Hello {{firstName}}, I noticed your expertise in {{techStack}} and your role as {{title}} at {{company}}. As someone also working in the tech space, I'd appreciate the opportunity to connect and potentially share insights about {{commonInterest}}.",
      "variables": ["firstName", "techStack", "title", "company", "commonInterest"],
      "successRate": 78.7,
      "usageCount": 850,
      "complianceScore": 92,
      "tags": ["technology", "specific", "personalized"],
      "industryTargets": ["Technology", "Software", "IT Services"]
    },
    "event_based": {
      "id": "connection_template_3",
      "name": "Conference Connection",
      "category": "event_based",
      "message": "Hi {{firstName}}, Great meeting you at {{eventName}}! I enjoyed our conversation about {{topic}}. Would love to stay connected and continue our discussion.",
      "variables": ["firstName", "eventName", "topic"],
      "successRate": 92.1,
      "usageCount": 425,
      "complianceScore": 98,
      "tags": ["event", "personal", "high_conversion"]
    },
    "mutual_connection": {
      "id": "connection_template_4",
      "name": "Mutual Connection Introduction",
      "category": "referral",
      "message": "Hi {{firstName}}, I noticed we're both connected to {{mutualConnection}}. I'm interested in your work in {{field}} and would love to connect. {{mutualConnection}} speaks highly of your expertise!",
      "variables": ["firstName", "mutualConnection", "field"],
      "successRate": 89.5,
      "usageCount": 650,
      "complianceScore": 94,
      "tags": ["referral", "mutual", "warm_intro"]
    },
    "content_engagement": {
      "id": "connection_template_5",
      "name": "Content-Based Connection",
      "category": "content_engagement",
      "message": "Hello {{firstName}}, I really enjoyed your recent post about {{postTopic}}. Your insights on {{specificPoint}} were particularly valuable. I'd love to connect and follow your content!",
      "variables": ["firstName", "postTopic", "specificPoint"],
      "successRate": 81.2,
      "usageCount": 380,
      "complianceScore": 93,
      "tags": ["content", "engagement", "thoughtful"]
    }
  },
  "engagementTemplates": {
    "likes": {
      "settings": {
        "enabled": true,
        "dailyLimit": 30,
        "targetTypes": ["post", "article", "video"],
        "industryFilter": ["Technology", "Marketing", "Sales"],
        "excludeCompetitors": true,
        "minEngagementThreshold": 5,
        "timeWindow": {
          "start": "09:00",
          "end": "17:00",
          "timezone": "America/New_York"
        }
      },
      "complianceRules": {
        "maxLikesPerProfile": 3,
        "minDelayBetweenLikes": 45,
        "maxDelayBetweenLikes": 180,
        "avoidBotDetection": true,
        "randomizeTimings": true
      }
    },
    "comments": {
      "templates": [
        {
          "id": "comment_template_1",
          "message": "Great insights! This really resonates with my experience in {{industry}}.",
          "variables": ["industry"],
          "category": "supportive",
          "successRate": 76.5
        },
        {
          "id": "comment_template_2",
          "message": "Thanks for sharing this, {{firstName}}. The point about {{keyTopic}} is particularly relevant right now.",
          "variables": ["firstName", "keyTopic"],
          "category": "appreciative",
          "successRate": 82.3
        },
        {
          "id": "comment_template_3",
          "message": "Interesting perspective on {{topic}}. Have you considered {{question}}?",
          "variables": ["topic", "question"],
          "category": "engaging",
          "successRate": 79.1
        },
        {
          "id": "comment_template_4",
          "message": "This aligns perfectly with what we're seeing in {{industry}} as well. Great post!",
          "variables": ["industry"],
          "category": "validating",
          "successRate": 74.8
        }
      ],
      "settings": {
        "enabled": true,
        "dailyLimit": 8,
        "minWordCount": 5,
        "maxWordCount": 25,
        "requirePersonalization": true,
        "avoidGenericComments": true
      },
      "complianceRules": {
        "maxCommentsPerProfile": 1,
        "minDelayBetweenComments": 120,
        "maxDelayBetweenComments": 300,
        "contentAnalysisRequired": true,
        "spamPrevention": true
      }
    },
    "profileViews": {
      "settings": {
        "enabled": true,
        "dailyLimit": 25,
        "targetCriteria": {
          "industries": ["Technology", "Marketing", "Sales", "Consulting"],
          "locations": ["United States", "Canada", "United Kingdom"],
          "connectionDegree": ["2nd", "3rd"],
          "experienceLevel": ["Mid-level", "Senior", "Director", "VP"]
        },
        "timeDistribution": {
          "morning": 30,
          "afternoon": 50,
          "evening": 20
        }
      },
      "complianceRules": {
        "maxViewsPerProfile": 1,
        "minDelayBetweenViews": 60,
        "maxDelayBetweenViews": 240,
        "respectPrivacySettings": true,
        "trackViewHistory": true
      }
    },
    "follows": {
      "settings": {
        "enabled": true,
        "dailyLimit": 5,
        "targetTypes": ["company_page", "thought_leader", "industry_influencer"],
        "relevanceScore": {
          "minimum": 7.0,
          "weightFactors": {
            "industryMatch": 0.4,
            "contentQuality": 0.3,
            "engagementRate": 0.2,
            "mutualConnections": 0.1
          }
        }
      },
      "complianceRules": {
        "maxFollowsPerDay": 5,
        "minDelayBetweenFollows": 180,
        "maxDelayBetweenFollows": 600,
        "qualityOverQuantity": true,
        "trackUnfollowRate": true
      }
    }
  },
  "safetySettings": {
    "globalLimits": {
      "dailyConnectionRequests": 15,
