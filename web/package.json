{
  "name": "@inergize/web-app",
  "version": "1.0.0",
  "description": "InErgize web application",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production node scripts/optimize-build.js",
    "lint:fix-all": "next lint --fix --dir pages --dir src",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "bundle:analyze": "npm run build:analyze && open bundle-analyzer-report.html",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless'",
    "perf:test": "npm run build && npm run start & sleep 5 && npm run perf:audit && kill %1",
    "test:e2e": "playwright test",
    "test:performance": "lighthouse http://localhost:3000 --preset=desktop --output=json --output-path=./performance-report.json"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
