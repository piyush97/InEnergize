    "@types/react-dom": "^19.0.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.0",
    "lighthouse": "^11.4.0",
